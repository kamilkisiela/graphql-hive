    "@sentry/node": "7.119.0",
    "@sentry/types": "7.119.0",
    "@tgriesser/schemats": "9.0.1",
    "@theguild/buddy": "0.1.0",
    "@types/node": "20.16.1",
    "@types/pg": "8.11.6",
    "dotenv": "16.4.5",
    "fast-json-stable-stringify": "2.1.0",
    "got": "14.4.2",
    "param-case": "3.0.4",
    "pg-promise": "11.9.1",
    "slonik": "30.4.4",
    "slonik-interceptor-query-logging": "46.1.0",
    "slonik-utilities": "1.9.4",
    "tslib": "2.7.0",
    "typescript": "5.5.4",
    "zod": "3.23.8"
